import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        int arr[]={2,3,4,4,6,6,9};
        System.out.print("{");
        System.out.print(arr[0]);
        for(int i=1;i<arr.length;i++){
            if(arr[i]!=arr[i-1]){
                System.out.print(","+arr[i]);
            }
        }
        System.out.print("}");
    }
}
