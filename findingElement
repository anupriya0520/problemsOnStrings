import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        int arr[]={2,3,4,5,6,7,9};
        int target=6;
        boolean found=false;
        for(int i=0;i<arr.length;i++){
            if(arr[i]==target){
               found=true;
               break;
            }
        }
        if(found){
            System.out.println("yes");
        }else
        {
            System.out.println("no");
        }
    }
}
